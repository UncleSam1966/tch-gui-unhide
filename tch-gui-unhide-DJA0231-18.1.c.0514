# Based on various pages https://hack-technicolor.readthedocs.io/en/stable/

SCRIPT="tch-gui-reveal"
TARGET_VARIANT="DJA0231"
TARGET_VERSION="18.1.c.0514"

# Use the device variant as the host name
VARIANT=$(uci get env.var.variant_friendly_name)
# Something more sensible than modem
DOMAIN=gateway

RESTORE=false

while getopts h:d:r option
do
 case "${option}" in
 h) VARIANT=${OPTARG};;
 d) DOMAIN=${OPTARG};;
 r) RESTORE=true;;
 esac
done

if [ $RESTORE = true ]
then
  echo [$SCRIPT]: Restore requested!! Enter y to undo changes:
  read
  if [ "$REPLY" = "y" -o "$REPLY" = "Y" ]
  then
    for d in $(echo cards docroot lua snippets | tr " " "$IFS")
    do
      for s in $(find /rom/www/$d -type f | grep -v -E \(/rom/www/docroot/help\))
      do
        if [ "$s" = "/rom/www/cards/010_lte.lp" ]
        then
          t="/www/cards/010_lte.lp"
        else
          t=$(echo "$s" | cut -c5-)
        fi
        cmp -s "$s" "$t"
        if [ $? -ne 0 ]
        then
          echo [$SCRIPT]: Restoring $t
          cp -f -p "$s" "$t"
        fi
      done
    done
    echo [$SCRIPT]: Removing additional files added to filesystem
    rm -f /www/docroot/img/favicon.ico /www/docroot/img/log-viewer /www/docroot/img/tcp-dump.png
    echo [$SCRIPT]: Removing config entries added to display hidden modals
    for s in $(uci show web | grep normally_hidden | cut -d. -f1,2)
    do
      uci delete $s
    done
    uci commit web
    /etc/init.d/nginx restart
    echo "************************************************************"
    echo "* Restore complete. You should clear your browser cache of *"
    echo "* images and files to make sure you can see the changes.   *"
    echo "* NOTE: No config changes have been restored, except to    *"
    echo "*       remove added entries to display hidden modals.     *"
    echo "************************************************************"
  fi
  exit
fi

VERSION=$(uci get version.@version[0].version | cut -d- -f1)
if [ "$VARIANT" = "$TARGET_VARIANT" -a "$VERSION" = "$TARGET_VERSION" ]
then
  echo [$SCRIPT]: Technicolor variant and version match target.
else
  echo [$SCRIPT]: WARNING: This script was tested on a Technicolor $TARGET_VARIANT with $TARGET_VERSION firmware.
  echo [$SCRIPT]: It MAY work on your $VARIANT with $VERSION firmware, but it is untested.
  echo [$SCRIPT]: If you still wish to proceed, reply FORCE \(in capitals as shown\).
  read
  if [ "$REPLY" != "FORCE" ]
  then
    echo [$SCRIPT]: Good choice.
    exit
  fi
fi

echo "[$SCRIPT]: This script will perform the following actions:"
echo "[$SCRIPT]:  - The host name will be set to:   $VARIANT"
echo "[$SCRIPT]:  - The domain name will be set to: $DOMAIN"
if [ -f authorized_keys ]
then
  echo "[$SCRIPT]:  - The local authorized_keys file will be copied to /etc/dropbear/authorized_keys"
else 
  echo "[$SCRIPT]:  - All keys will be removed from /etc/dropbear/authorized_keys"
  echo "[$SCRIPT]:   \(If you wish to automatically apply your own public keys, run this script from the directory containing your authorized_keys file\)"
fi
echo "[$SCRIPT]:  - SSH LAN access will be enabled"
echo "[$SCRIPT]:  - SSH WAN access will be disabled"
echo "[$SCRIPT]:  - CWMP will be disabled"
echo "[$SCRIPT]:  - Telstra monitoring will be disabled"
echo "[$SCRIPT]:  - Telstra AIR hotspot will be disabled and the associated SSIDs removed"
echo "[$SCRIPT]:  - Configuration export/import and firmware upgrade will be enabled in the GUI"
echo "[$SCRIPT]:  - The following screens which are normally hidden will be visible:"
echo "[$SCRIPT]:    . cwmpconf-modal.lp"
echo "[$SCRIPT]:    . diagnostics-tcpdump-modal.lp"
echo "[$SCRIPT]:    . iproutes-modal.lp"
echo "[$SCRIPT]:    . log-connections-modal.lp"
echo "[$SCRIPT]:    . log-routingtable-modal.lp"
echo "[$SCRIPT]:    . lte-sms.lp"
echo "[$SCRIPT]:    . mmpbx-global-modal.lp"
echo "[$SCRIPT]:    . mmpbx-inoutgoingmap-modal.lp"
echo "[$SCRIPT]:    . mmpbx-profile-modal.lp"
echo "[$SCRIPT]:    . mmpbx-sipdevice-modal.lp"
echo "[$SCRIPT]:    . mmpbx-statistics-modal.lp"
echo "[$SCRIPT]:    . nat-alg-helper-modal.lp"
echo "[$SCRIPT]:    . system-modal.lp"
echo "[$SCRIPT]:    . tod_wireless-modal.lp"
echo "[$SCRIPT]:    . xdsl-low-modal.lp"
echo "[$SCRIPT]:  - The GUI screen will be prettied up a bit."
echo "[$SCRIPT]:  - DumaOS will be enabled."

echo [$SCRIPT]: If you wish to proceed, enter y otherwise just press [Enter] to stop and re-run with the -h and/or -d parameters.
read
if [ "$REPLY" != "y" -a "$REPLY" != "Y" ]
then
  echo [$SCRIPT]: Optional parameters are -h hostname -d domainname
  exit
fi

echo [$SCRIPT]: IMPORTANT NOTE - You can restore changed files to their original state by running: $SCRIPT -r

echo [$SCRIPT]: Setting the host name to $VARIANT
uci -q delete dhcp.main.hostname
uci -q delete dhcp.guest.hostname

uci add_list dhcp.main.hostname="$VARIANT"
uci add_list dhcp.guest.hostname="$VARIANT"
uci set system.@system[0].hostname="$VARIANT"
export HOSTNAME=$VARIANT

echo [$SCRIPT]: Setting the domain to $DOMAIN
uci set dhcp.main.domain="$DOMAIN"
uci set dhcp.guest.domain="$DOMAIN"

sed -e "s^http://mymodem.modem^http://$VARIANT.$DOMAIN^" -i /etc/nginx/nginx.conf

echo [$SCRIPT]: Fixing SSH access
if [ -f authorized_keys ]
then
  echo [$SCRIPT]: Found authorized_keys - replacing /etc/dropbear/authorized_keys
  cp authorized_keys /etc/dropbear/authorized_keys
else
  echo [$SCRIPT]: Removing all public keys from /etc/dropbear/authorized_keys
  echo > /etc/dropbear/authorized_keys
fi
echo [$SCRIPT]: Enable SSH access over LAN
# We need to enable this properly as you can enable/disable via the GUI, and if done
# without allowing password auth and root login, it can accidently prevent SSH access
uci set dropbear.lan.enable='1'
uci set dropbear.lan.PasswordAuth='on'
uci set dropbear.lan.RootPasswordAuth='on'
uci set dropbear.lan.RootLogin='1'

echo [$SCRIPT]: Completely disable SSH access over WAN
uci set dropbear.wan.enable='0'

echo [$SCRIPT]: Disabling CWMP
uci set cwmpd.cwmpd_config.state=0

echo [$SCRIPT]: Disabling Telstra monitoring
uci -q delete autoreset.vsparc_enabled
uci -q delete autoreset.thor_enabled
uci -q delete wifi_doctor_agent.acs
uci -q delete wifi_doctor_agent.config
uci -q delete wifi_doctor_agent.as_config

echo [$SCRIPT]: Removing Telstra Air
uci -q delete dhcp.hotspot
uci -q delete dhcp.fonopen
uci -q delete network.fonopen
uci -q delete web.fon
uci -q del_list qos.reclassify_1.srcif='fonopen'
for wl in $(uci show wireless | grep -E ssid=\'\(Fon\|Telstra\ Air\) | cut -d. -f2)
do
  ap=$(uci show wireless | grep iface=\'$wl\' | cut -d. -f2)
  echo [$SCRIPT]: Removing SSID $wl \(AP=$ap\)
  uci delete wireless.$wl
  uci delete wireless.$ap
done
if [ -f /www/cards/010_fon.lp ]
then
  rm /www/cards/010_fon.lp
fi

echo [$SCRIPT]: Enabling Unsigned config export and Firmware upgrade in Web GUI
uci set system.config.export_plaintext='1'
uci set system.config.export_unsigned='1'
uci set system.config.import_plaintext='1'
uci set system.config.import_unsigned='1'
uci set web.uidefault.upgradefw='1'
uci set web.uidefault.upgradefw_role='admin'
uci -q del_list web.parentalblock.roles='admin'
uci add_list web.parentalblock.roles='admin'

uci commit

# Check all modals are enabled, except:
#  - bridge-grouping-modal.lp (fails with a bad argument error)
#  - diagnostics-airiq-modal.lp (requires Flash player)
#  - fon-modal.lp (Telstra Air)
#  - hostmap-modal.lp
#  - mwan-modal.lp (tries to include "tabs-internet.lp" which does not exist) 
#  - relay-modal.lp (fails with nil reference)
#  - speedservice-modal.lp
#  - wireless-qrcode-modal.lp (fails with a nill password index error)
for f in $(ls /www/docroot/modals | grep -E -v \(bridge-grouping-modal.lp\|diagnostics-airiq-modal.lp\|fon-modal.lp\|hostmap-modal.lp\|mwan-modal.lp\|relay-modal.lp\|speedservice-modal.lp\|wireless-qrcode-modal.lp\) )
do
  uci show web | grep -q "/modals/$f"
  if [ $? -eq 1 ]
  then
      echo [$SCRIPT]: Enabling $f
      RULE=$(basename $f .lp | sed -e 's/-//g')
      uci add_list web.ruleset_main.rules=$RULE
      uci set web.$RULE=rule
      uci set web.$RULE.target=/modals/$f
      uci set web.$RULE.normally_hidden='1'
      uci add_list web.$RULE.roles=admin
  fi
done
uci commit web

echo [$SCRIPT]: Enable DumaOS
uci set dumaos.tr69.dumaos_enabled='1'
uci commit dumaos

echo [$SCRIPT]: Change the title in browser tabs from Modem or Gateway to $VARIANT
sed -e "s/\(Modem\|Gateway\)/$VARIANT/g" -i /www/lua/telstra_helper.lua

echo [$SCRIPT]: Restore any changed files from /rom/www
for d in $(echo cards docroot | tr " " "$IFS")
do
  for s in $(find /rom/www/$d -type f | grep -v -E \(/rom/www/docroot/help\|010_lte.lp\|010_fon.lp\))
  do
    t=$(echo "$s" | cut -c5-)
    cmp -s "$s" "$t"
    if [ $? -ne 0 ]
    then
      cp -f -p "$s" "$t"
    fi
  done
done

if [ -f ipv4-DNS-Servers ]
then
  echo [$SCRIPT]: Adding custom IPv4 DNS Servers
  sed -e '/^  },/,$ d' /www/docroot/modals/ethernet-modal.lp > /www/docroot/modals/ethernet-modal.lp.tmp
  while read -r host ip
  do 
    if [ ! -z "$ip" ]
    then 
      echo "    {\"$ip\", T\"$host ($ip)\"}," >> /www/docroot/modals/ethernet-modal.lp.tmp
    fi
  done < ipv4-DNS-Servers
  echo "  }," >> /www/docroot/modals/ethernet-modal.lp.tmp
  sed -e '1,/^  },/ d' /www/docroot/modals/ethernet-modal.lp >> /www/docroot/modals/ethernet-modal.lp.tmp
  mv /www/docroot/modals/ethernet-modal.lp.tmp /www/docroot/modals/ethernet-modal.lp
fi

echo [$SCRIPT]: Change config export filename from config.bin to $VARIANT-$VERSION-YYMMDD.bin
sed -e "s/=config.bin/=$VARIANT-$VERSION-\" .. os.date(\"%Y%m%d\") .. \".bin/" -i /www/docroot/modals/gateway-modal.lp

echo [$SCRIPT]: Add DumaOS button
sed -e '/id="basicview"/,$ d' /rom/www/docroot/gateway.lp > /www/docroot/gateway.lp.tmp
echo "            html[#html + 1] = '<a href=\"desktop/index.html#com.netdumasoftware.desktop\" class=\"btn\" id=\"dumaos\">'" >> /www/docroot/gateway.lp.tmp
echo '            html[#html + 1] = T"DumaOS"' >> /www/docroot/gateway.lp.tmp
echo "            html[#html + 1] = '</a>'" >> /www/docroot/gateway.lp.tmp
sed -e '1,/if not bridged.isBridgedMode/ d' /rom/www/docroot/gateway.lp >> /www/docroot/gateway.lp.tmp
mv /www/docroot/gateway.lp.tmp /www/docroot/gateway.lp

echo [$SCRIPT]: Enable various things that only the guest role\(!!!\) was allowed to do or see
for f in $(ls /www/docroot/modals/gateway-modal.lp /www/docroot/modals/internet-modal.lp /www/docroot/modals/wireless-modal.lp /www/docroot/modals/wanservices-modal.lp /www/snippets/tabs-diagnostics.lp)
do
  sed -e 's/\(if [^ ]*role[^=]*==[^"]*"\)\(guest\)\("\)/\1admin\3/g' -i $f
done

echo [$SCRIPT]: Show the hidden Telephony tabs
sed \
 -e 's/--  {"mmpbx-contacts/    {"mmpbx-contacts/' \
 -e 's/--  {"mmpbx-service-modal.lp", T"Service"},/    {"mmpbx-service-modal.lp", T"Service"},\n    {"mmpbx-sipdevice-modal.lp", T"SIP Device"},\n    {"mmpbx-inoutgoingmap-modal.lp", T"In\/Outgoing Map"},\n    {"mmpbx-statistics-modal.lp", T"Statistics"},/' \
 -i /www/snippets/tabs-voice.lp
echo [$SCRIPT]: Change default Telephony tab to Global
# Default telephony tab is DECT!!! 
# - Set test to always be false (mmpbx_state can only be 0 or 1, so 9 should always be false) 
#   to invoke alternate processing branches and show global or info first.
sed -e 's/if mmpbx_state == "1"/if mmpbx_state == "9"/' -i /www/cards/008_telephony.lp

echo [$SCRIPT]: Add missing icons on Diagnostics card
# Show the icons for all tabs on the card
# (Nicer and extra icons are below...)
sed -e 's^\(alt="ping/trace"></div></td>\)\(</tr>\\\)^\1\\\n <td><div data-toggle="modal" data-remote="modals/logviewer-modal.lp" data-id="logviewer-modal"><img href="#" rel="tooltip" data-original-title="LOGVIEWER" src="/img/log-viewer.png" alt="logviewer"></div></td>\2^' -i /www/cards/009_diagnostics.lp
sed -e 's^\(alt="network"></div></td>\)\(</tr>\\\)^\1\\\n <td><div data-toggle="modal" data-remote="modals/diagnostics-tcpdump-modal.lp" data-id="diagnostics-tcpdump-modal"><img href="#" rel="tooltip" data-original-title="TCPDUMP" src="/img/tcp-dump.png" alt="tcpdump"></div></td>\2^' -i /www/cards/009_diagnostics.lp

echo [$SCRIPT]: Add cogs to card headers
for f in $(grep -l createCardHeaderNoIcon /www/cards/*)
do
  sed -e 's/createCardHeaderNoIcon/createCardHeader/' -i $f
done
sed -e 's/switchName, content\["cwmp_state"\], {input = {id = "cwmp_card_state"}}/nil, nil, nil/' -i /www/cards/090_cwmpconf.lp

echo [$SCRIPT]: Remove the background card icons from the cards that do have them
sed -e 's/class="content card_bg" data-bg-text="\&#xf0ad;"/class="content"/' -i /www/cards/091_system.lp
sed -e 's/class="content card_bg" data-bg-text="\&#xf0ad;"/class="content"/' -i /www/cards/092_natalghelper.lp
sed -e 's/class="content card_bg" data-bg-text="\&#xf0ad;"/class="content"/' -i /www/cards/093_xdsl.lp

echo [$SCRIPT]: Apply the CSS changes
sed \
 -e '$ a a{color:#404040;}' \
 -e '$ a body{background-color:#fff;font-family:'Roboto','Noto',sans-serif;}' \
 -e '$ a input,button,select,textarea{font-family:'Roboto','Noto',sans-serif;}' \
 -e '$ a u{text-decoration:none;}' \
 -e '$ a .card{font-family:'Roboto','Noto',sans-serif;}' \
 -e '$ a .copyright{font-family:'Roboto','Noto',sans-serif;text-shadow:none;display:none;}' \
 -e '$ a .dw{font-family:'Roboto','Noto',sans-serif;}' \
 -e '$ a .header img{width:180px;}' \
 -e '$ a .header .btn{margin-left:5px;}' \
 -e '$ a .modal-backdrop{background-color:#fff;}' \
 -e '$ a .modal-header{color:#404040;}' \
 -e '$ a .nav-header{text-shadow:none;}' \
 -e '$ a .nav-list>li>a,.nav-list .nav-header{text-shadow:none;}' \
 -e '$ a .nav-tabs>.active>a, .nav-tabs>.active>a:hover, .nav-tabs>.active>a:focus{background-color:#00B1EB;color:#fff}' \
 -e '$ a .simple-desc{display:block;}' \
 -e '$ a .smallcard{border:0px;background-color:#eee;font-family:'Roboto','Noto',sans-serif;font-size:14px;font-weight:400;color:rgba(64,64,64,0.75);}' \
 -e '$ a .smallcard .content{font-size:14px;}' \
 -e '$ a .smallcard .header{background-color:#eee;background-image:none;border-bottom:0px;color:#404040;font-size:20px;font-weight:400;text-shadow:none;}' \
 -e '$ a .smallcard .subinfos{font-size:14px;color:rgba(64,64,64,0.75);}' \
 -i /www/docroot/css/gw.css
sed -e '$ a .navbar-inverse .navbar-inner{background-color:#fff;background-image:none;border-width:0px;}' -i /www/docroot/css/gw-telstra.css
sed -e 's/margin-left:70px/margin-left:190px/' -i /www/docroot/css/mobiled.css

echo [$SCRIPT]: Add a favicon
for f in $(grep -l '<head>\\' /www/docroot/*.lp)
do
  sed -e 's^<head>\\^<link rel="shortcut icon" type="image/x-icon" href="/img/favicon.ico"/>\\^' -i $f
done
